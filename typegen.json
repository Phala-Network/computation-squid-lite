{
  "$schema": "https://raw.githubusercontent.com/subsquid/squid-sdk/master/substrate/substrate-typegen/src/config.schema.json",
  "outDir": "src/types",
  "specVersions": "https://v2.archive.subsquid.io/metadata/khala",
  "pallets": {
    "PhalaComputation": {
      "events": [
        "WorkerStarted",
        "WorkerStopped",
        "WorkerReclaimed",
        "SessionBound",
        "SessionUnbound",
        "WorkerEnterUnresponsive",
        "WorkerExitUnresponsive",
        "SessionSettled",
        "BenchmarkUpdated"
      ]
    },
    "PhalaRegistry": {
      "events": ["WorkerAdded", "WorkerUpdated", "InitialScoreSet"]
    }
  }
}
